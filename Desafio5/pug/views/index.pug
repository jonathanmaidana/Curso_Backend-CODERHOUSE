html 
    head
        body 
            h1 Formulario
            form(method="POST" action='http://localhost:3000/productos')
                label= 'producto'
                input(type="text" name='titulo' placeholder="titulo")
                label= 'precio'
                input(type="text" name='precio' placeholder="precio")
                input(type='submit')

            each val in listaProductos 
                div=val.title
                div=val.precio